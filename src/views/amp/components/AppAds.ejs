<amp-ad
  class="app-ads"
  width="<%= _.get(AMPAds, [ 'DFPUnits', pos, 'dimensions', 'width' ], 0) %>"
  height="<%= _.get(AMPAds, [ 'DFPUnits', pos, 'dimensions', 'height' ], 0) %>"
  type="doubleclick"
  data-slot="/<%= AMPAds.DFP_ID %>/<%= _.get(AMPAds, [ 'DFPUnits', pos, 'aduid' ], '') %>"
  data-multi-size="<%= _.get(AMPAds, [ 'DFPUnits', pos, 'multiSize' ], '300x250') %>"
  data-multi-size-validation="false"
>
</amp-ad>