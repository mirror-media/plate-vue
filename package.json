    "bowser": "^1.9.4",
    "compression": "^1.7.3",
    "cookies": "^0.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "debug": "^4.1.0",
    "double-ended-queue": "^2.1.0-0",
    "ejs": "^2.6.1",
    "es6-promise": "^4.2.5",
    "express": "^4.16.4",
    "forever": "^0.15.3",
    "humps": "^2.0.0",
    "iframe-resizer": "^3.6.3",
    "jshashtable": "^0.1.0",
    "kc-scroll": "^0.2.71",
    "lodash": "^4.17.11",
    "lru-cache": "^4.1.3",
    "moment": "^2.22.2",
