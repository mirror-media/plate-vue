    "superagent": "^5.2.1",
    "tap.js": "^2.2.0",
    "truncate": "^2.1.0",
    "uuid": "^3.3.3",
    "verge": "^1.10.2",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^3.1.3",
    "vue-client-only": "^2.0.0",
    "vue-cookie": "^1.1.3",
