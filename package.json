{
  "name": "plate-vue",
  "description": "A Vue.js project",
  "author": "MirrorMedia <master@mirrormedia.mg>",
  "private": true,
  "scripts": {
    "api": "node test/api/runner.js --timeout 5000 test/api/specs/*.spec.js",
    "dev": "npm run npm-forever",
    "start": "cross-env NODE_ENV=production PORT=80 npm run npm-forever",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "e2e": "node test/e2e/runner.js",
    "lint": "eslint --ext .js,.vue amp api build src server* --color",
    "npm-forever": "forever -o out.log -e err.log -c 'node --expose-gc' serverCluster.js",
    "test": "jest tests/unit/util/comm.test.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "unit-watch": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    "validate": "npm ls"
  },
  "engines": {
    "node": ">=10.18.0",
    "npm": ">=6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupTestFrameworkScriptFile": "jest-extended"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "@google-cloud/logging": "^7.0.0",
    "@google-cloud/profiler": "^3.0.1",
    "@readr-ui/embedded-promotions": "^1.2.2",
    "big-integer": "^1.6.48",
    "body-parser": "^1.19.0",
    "bowser": "^2.8.1",
    "compression": "^1.7.4",
    "cookies": "^0.8.0",
    "core-js": "^3.6.4",
    "cross-env": "^6.0.3",
    "debug": "^4.1.1",
    "double-ended-queue": "^2.1.0-0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "forever": "^2.0.0",
    "humps": "^2.0.0",
    "ioredis": "^4.14.1",
    "jshashtable": "^0.1.0",
    "kc-scroll": "^0.2.71",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "moment": "^2.24.0",
    "node-memwatch": "^1.0.1",
