{
    "extends": [
      "eslint:recommended",
      "plugin:vue/base"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
        "import"
    ],
    "rules": { 
      "array-bracket-spacing": [ 2, "always" ],
      "camelcase": [ 0, "" ],
      "no-case-declarations": 1,
      "no-console": 1,
      "no-useless-escape": 1,
      "yoda": [ 0, "never" ]
    },
    "globals": {
      "location": true,
      "source": true
    },
    "env": {
      "node": true,
      "jasmine": true
    }
}